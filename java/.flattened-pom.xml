<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.totvs.tjf</groupId>
    <artifactId>tjf-boot-starter</artifactId>
    <version>2.6.0-RELEASE</version>
    <relativePath></relativePath>
  </parent>
  <groupId>com.totvs.agro</groupId>
  <artifactId>price-component</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>PriceComponent</name>
  <description>Demo project for Spring Boot</description>
  <dependencies>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-data-jpa</artifactId>
      <version>2.3.1.RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-web</artifactId>
      <version>2.3.1.RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.totvs.tjf</groupId>
      <artifactId>tjf-api-core</artifactId>
      <version>2.6.0-RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.totvs.tjf</groupId>
      <artifactId>tjf-api-jpa</artifactId>
      <version>2.6.0-RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>mysql</groupId>
      <artifactId>mysql-connector-java</artifactId>
      <version>5.1.49</version>
      <scope>runtime</scope>
      <exclusions>
        <exclusion>
          <artifactId>protobuf-java</artifactId>
          <groupId>com.google.protobuf</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.12</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-test</artifactId>
      <version>2.3.1.RELEASE</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>junit-vintage-engine</artifactId>
          <groupId>org.junit.vintage</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.totvs.tjf</groupId>
      <artifactId>tjf-core-common</artifactId>
      <version>2.6.0-RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.totvs.tjf</groupId>
      <artifactId>tjf-i18n-core</artifactId>
      <version>2.6.0-RELEASE</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
</project>
